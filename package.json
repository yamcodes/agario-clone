{
  "name": "agario-clone",
  "version": "0.3.1",
  "description": "The easily tweakable clone for modders, developers and .io games enthusiasts of any level!",
  "main": "game.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "npx watchify game.js -o game-bundled.js -p esmify ",
